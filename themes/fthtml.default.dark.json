{
"$schema": "vscode://schemas/color-theme",
"name": "Dark+ (default dark & ftHTML)",
"include": "./dark_vs.json",
"tokenColors": [
    {
      "name" : "variable.other.fthtml",
      "scope" : "variable.other.fthtml",
      "settings" : {
        "foreground" : "#C586C0",
        "fontStyle" : "italic"
      }
    },
    {
      "name": "directives",
      "scope": "keyword.control.directive.fthtml",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "name": "entity punctuation",
      "scope":"punctuation.definition.tag.begin.fthtml, punctuation.definition.tag.end.fthtml",
      "settings": {
        "foreground": "#808080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "keyword.other.fthtml",
      "scope": "keyword.other.template.fthtml",
      "settings": {
        "foreground": "#46B49E",
        "fontStyle": "bold"
      }
    },
    {
      "name" : "keyword.other.fthtml",
      "scope" : "keyword.other.import.fthtml, keyword.other.comment.fthtml,keyword.other.doctype.fthtml",
      "settings" : {
        "foreground" : "#C1A770",
        "fontStyle": "bold"
      }
    },
    {
      "name": "strings",
      "scope": "string.quoted.single.fthtml, string.quoted.double.fthtml, string.unquoted.fthtml",
      "settings" : {
        "foreground" : "#CE9178"
      }
    },
    {
      "name": "comments",
      "scope": "comment.line.fthtml, comment.block.fthtml",
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "name": "tags",
      "scope": "entity.name.tag.fthtml",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name" : "tag punctuation",
      "scope": "punctuation.separator.plus.fthtml, punctuation.separator.key-value.fthtml, punctuation.definition.entity.other.attribute-group.begin.fthtml, punctuation.definition.entity.other.attribute-group.end.fthtml",
      "settings": {
        "foreground":"#9CDCFE"
      }
    },
    {
      "name": "entities",
      "scope" : "entity.other.attribute-name.fthtml, entity.other.attribute-name.class.fthtml, entity.other.attribute-name.id.fthtml",
      "settings": {
        "foreground": "#9cdcfe"
      }
    }
  ]
}